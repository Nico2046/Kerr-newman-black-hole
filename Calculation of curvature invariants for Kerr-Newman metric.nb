Cell[TextData[Cell[BoxData[
 FormBox[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{
        SubsuperscriptBox["\[CapitalGamma]", "\[Beta]\[Gamma]", "\[Alpha]"], 
        "\[LongEqual]", 
        FractionBox["1", "2"], 
        SuperscriptBox[
         StyleBox["g", "TI"], "\[Alpha]\[Lambda]"], 
        RowBox[{"{", 
          RowBox[{
            SubscriptBox["\[PartialD]", "\[Gamma]"], 
            SubscriptBox[
             StyleBox["g", "TI"], "\[Lambda]\[Beta]"], "+", 
            SubscriptBox["\[PartialD]", "\[Beta]"], 
            SubscriptBox[
             StyleBox["g", "TI"], "\[Lambda]\[Gamma]"], "-", 
            SubscriptBox["\[PartialD]", "\[Lambda]"], 
            SubscriptBox[
             StyleBox["g", "TI"], "\[Beta]\[Gamma]"]}], "}"}]}], 
      TraditionalForm], "errors" -> {}, "input" -> 
    "\\Gamma^\\alpha_{\\beta\\gamma} = \\frac{1}{2} g^{\\alpha\\lambda} \
\\left\\{ \\partial_{\\gamma} g_{\\lambda \\beta} + \\partial_{\\beta} \
g_{\\lambda \\gamma} - \\partial_{\\lambda} g_{\\beta \\gamma} \\right\\}", 
    "state" -> "Boxes"|>,
   "TeXAssistantTemplate"], 
  TraditionalForm]],ExpressionUUID->"c962dfc1-728b-43e2-8617-8ef72e3e9f92"]], \
